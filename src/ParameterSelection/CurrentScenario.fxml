<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.SubScene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <SubScene fx:id="parachutePane" height="599.0" width="800.0">
        <root>
          <Region />
        </root>
      </SubScene>
      <SubScene fx:id="drumPane" height="599.0" width="800.0">
        <root>
          <Region />
        </root>
      </SubScene>
      <SubScene fx:id="gliderPane" height="599.0" width="800.0">
         <root>
            <Region />
         </root>
      </SubScene>
      <SubScene fx:id="pilotPane" height="599.0" width="800.0">
         <root>
            <Region />
         </root>
      </SubScene>
      <SubScene fx:id="winchPosPane" height="599.0" width="800.0">
         <root>
            <Region />
         </root>
      </SubScene>
      <SubScene fx:id="gliderPosPane" height="599.0" width="800.0">
         <root>
            <Region />
         </root>
      </SubScene>
      <SubScene fx:id="runwayPane" height="599.0" width="800.0">
         <root>
            <Region />
         </root>
      </SubScene>
      <SubScene fx:id="airfieldPane" height="599.0" width="800.0">
         <root>
            <Region />
         </root>
      </SubScene>
      <GridPane fx:id="scenarioHomePane" prefHeight="599.0" prefWidth="800.0" style="-fx-background-color: white;">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Group GridPane.halignment="RIGHT" GridPane.valignment="BOTTOM">
               <children>
                  <Rectangle fx:id="pilotGridBackground" arcHeight="5.0" arcWidth="5.0" fill="#dfe0e1" height="250.0" stroke="BLACK" strokeType="INSIDE" width="344.0" />
                  <Label layoutX="5.0" layoutY="-2.0" text="Pilot">
                     <font>
                        <Font size="48.0" />
                     </font>
                  </Label>
                  <FlowPane alignment="CENTER" columnHalignment="CENTER" layoutX="5.0" layoutY="60.0" orientation="VERTICAL" prefHeight="182.0" prefWidth="335.0">
                     <children>
                        <Label text="Pilot Name" />
                        <Button fx:id="pilotButton" mnemonicParsing="false" onAction="#PilotButton_Click" text="Select Pilot" />
                     </children>
                  </FlowPane>
               </children>
            </Group>
            <Group GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.valignment="BOTTOM">
               <children>
                  <Rectangle fx:id="gliderGridBackground" arcHeight="5.0" arcWidth="5.0" fill="#dfe0e1" height="250.0" layoutY="2.0" stroke="BLACK" strokeType="INSIDE" width="345.0" />
                  <Label layoutX="210.0" layoutY="-2.0" text="Glider">
                     <font>
                        <Font size="48.0" />
                     </font>
                  </Label>
                  <FlowPane alignment="CENTER" columnHalignment="CENTER" layoutX="5.0" layoutY="60.0" orientation="VERTICAL" prefHeight="182.0" prefWidth="335.0">
                     <children>
                        <Label text="N-Number" />
                        <Label text="Total Weight" />
                        <Button fx:id="gliderButton" mnemonicParsing="false" onAction="#GliderButton_Click" text="Select Glider" />
                     </children>
                  </FlowPane>
               </children>
            </Group>
            <Group GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="TOP">
               <children>
                  <Rectangle fx:id="runDescriptionGridBackground" arcHeight="5.0" arcWidth="5.0" fill="#dfe0e1" height="250.0" stroke="BLACK" strokeType="INSIDE" width="345.0" />
                  <Label layoutX="3.0" layoutY="186.0" text="Run Description">
                     <font>
                        <Font size="48.0" />
                     </font>
                  </Label>
                  <FlowPane alignment="CENTER" columnHalignment="CENTER" layoutX="5.0" layoutY="7.0" orientation="VERTICAL" prefHeight="182.0" prefWidth="335.0">
                     <children>
                        <Label text="Airfield Name  -   Designator" />
                        <Label text="Magnetic Heading" />
                        <Label text="Glider Position" />
                        <Label text="Winch Position" />
                        <Button fx:id="airfieldButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#EditAirFieldButton_Click" text="Select Run Description" />
                     </children>
                  </FlowPane>
               </children>
            </Group>
            <Group GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP">
               <children>
                  <Rectangle fx:id="drumGridBackground" arcHeight="5.0" arcWidth="5.0" fill="#dfe0e1" height="250.0" stroke="BLACK" strokeType="INSIDE" width="345.0" />
                  <Label layoutX="221.0" layoutY="186.0" text="Drum">
                     <font>
                        <Font size="48.0" />
                     </font>
                  </Label>
                  <FlowPane alignment="CENTER" columnHalignment="CENTER" layoutX="5.0" layoutY="7.0" orientation="VERTICAL" prefHeight="182.0" prefWidth="335.0">
                     <children>
                        <Label text="Drum Info" />
                        <Label text="Parachute Info" />
                        <Button fx:id="drumButton" mnemonicParsing="false" onAction="#DrumButton_Click" text="Select Drum" />
                     </children>
                  </FlowPane>
               </children>
            </Group>
         </children>
      </GridPane>
   </children>
</AnchorPane>
